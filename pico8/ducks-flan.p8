-- map code

function init_map()
	map_items_t = {
		traps = 0
	}
	flags = {
		wall=0,
		trap=1,
		active_trap=4,
		text_panel=2,
		flan=7,
	}
end

function update_map()
	update_traps()
end

function draw_map()
	map(mapx,mapy)
end	

function mhas_flag(flag,x,y)
	local sprite = mget(mapx+x,mapy+y)
	return fget(sprite, flag)
end

function switch_sprite(x,y)
	local cur_s = mget(x,y)
	mset(x,y,cur_s+1)
end

function unswitch_sprite(x,y)
	local cur_s = mget(x,y)
	mset(x,y,cur_s-1)
end

function update_traps()
	if map_items_t.traps == flr(trap_speed/3) then
--		sfx(8)
	end
	if map_items_t.traps == flr(trap_speed/2) then
		sfx(2)
	end
	if map_items_t.traps == flr(trap_speed* 2/3) then
--		sfx(7)
	end
	if map_items_t.traps == trap_speed then
		sfx(1)
		map_items_t.traps = 0
		for x=mapx,mapx+15 do
			for y=mapy,mapy+15 do
				local s = mget(x,y)
				if fget(s, flags.trap) then
					if fget(s,flags.active_trap) then
						switch_sprite(x,y)
					else
						unswitch_sprite(x,y)
					end
				end
			end
		end
	end
	map_items_t.traps += 1
end
