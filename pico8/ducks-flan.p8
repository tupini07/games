-- text panel code

function text_panel_init()
	textsp = {}
	textsp_current = nil
	
	add_text(1,8,1,"welcome!\ntry to reach the\ndelecious flan on the\nother side of the river")
	add_text(5,6,2,"this is an easy one...")
	add_text(3,3,3,"that was easy, wasn't\nit?\n\nlet's make it a bit\nharder.")
	add_text(3,2,4,"let's try something\na\n\n    bit\n\n        more...\n\n            diagonal.")
	add_text(2,4,5,"can you sprint?")
	add_text(3,13,6,"well done!")
	add_text(1,0,7,"oh no!\n\nquick grab the last\nflan before it's too\nlate")
	add_text(0,6,8,"thanks for playing ♥!\n\nnow, enjoy your flans")
end

function text_panel_update()
	if textsp_current != nil and btnp(❎) then
		textsp_current = nil
	end
end

function text_panel_draw()
	if textsp_current != nil then
		rectfill(10,45,117,117,7)
		print(textsp_current.text,20,55,5)
		print("press ❎ to close.",20,107,6)
	end
end

function add_text(x,y,lvl,text)
	add(textsp, {
		x=x,y=y,lvl=lvl,text=text
	})
end

function show_text(x,y)
	for t in all(textsp) do
		if t.lvl == level_num and t.x == x and t.y == y then
			sfx(9)
			textsp_current = t
			return
		end
	end
end
-->8
-- level code

function level_set(num)
	mapx = (num-1)*16
	mapy = 0
	
	level_num = num
	game_over = false
	player_win = false
	
	trap_speed = 30
	
	
	game_completed = false
	if num > 8 then
		game_over = true
		game_completed = true
	end
	if (pl != nil) position_player_in_lvl()
end

function gamestate_update()
	if game_over and btnp(❎) then
		if player_win then
			level_set(level_num+1)
		else
			level_set(level_num)
		end
	end
end

function gamestate_draw()
	if game_over then
		if player_win then
			rectfill(0,50,127,70,7)
			line(0,51,127,51,10)
			line(0,69,127,69,10)
			print("you win!",55,55,5)
			print("press ❎ for next level",15,63,5)
		elseif game_completed then
			cls(3)
			pal(5,7)
			print(" you beat the game",0,32)
			print(" there aren't any more levels")
			print(" thanks for playing ♥")
			pal()
			palt(6,true)
			sspr(8,16,8,8,25,48,8*10,8*10)
			palt(6,false)
		else
			rectfill(0,50,127,70,0)
			line(0,51,127,51,8)
			line(0,69,127,69,8)
			print("you lose!",55,55,5)
			print("press ❎ to restart level",15,63,5)
		end
	end
end

function position_player_in_lvl()
	if level_num == 1 then
		pl.x=1 pl.y=5
	elseif level_num == 2 then
		pl.x=4 pl.y=5
	elseif level_num == 3 then
		pl.x=2 pl.y=2
	elseif level_num == 4 then
		pl.x=2 pl.y=2
	elseif level_num == 5 then
		pl.x=2 pl.y=3
	elseif level_num == 6 then	
		pl.x=2 pl.y=2
	elseif level_num == 7 then
		pl.x=1 pl.y=1
	elseif level_num == 8 then
		pl.x=0 pl.y=7
	end
end
