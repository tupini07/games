-- duck's flan
-- by dadum

function _init()
	player_init()
	level_set(1)
	init_map()
	text_panel_init()
end

function _update()
	text_panel_update()
	
	if not game_over then
		player_update()
		update_map()
	end
	
	gamestate_update()
end

function _draw()
	cls()
	draw_map()
	player_draw()
	text_panel_draw()
	gamestate_draw()
end
