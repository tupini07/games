-- player code

function player_init()
	pl = {
		x=1, y=5, dir=1
	}
end

function player_update()
	local x,y = get_next_x_y()

	interact(x,y)
	
	if pl.x != x or pl.y != y then
		pl.dir = sgn(x-pl.x)
		if not mhas_flag(flags.wall,x,y) then
			sfx(4)
			pl.x = x
			pl.y = y
		else
			sfx(10)
		end
	end

	check_traps()
	check_flan()
end

function player_draw()
	spr(1,pl.x*8,pl.y*8,1,1,pl.dir==-1)
end

function get_next_x_y()
	local nx, ny = pl.x,pl.y
	if (btnp(⬆️)) ny = pl.y-1 
	if (btnp(⬇️)) ny = pl.y+1
	if (btnp(⬅️)) nx = pl.x-1
	if (btnp(➡️)) nx = pl.x+1
	
	nx = mid(0,nx,15)
	ny = mid(0,ny,15)
	return nx,ny
end

function interact(x,y)
	if mhas_flag(flags.text_panel,x,y) then
		show_text(x,y)
	end
end

function check_traps()
	local is_active_trap = mhas_flag(flags.active_trap,pl.x,pl.y)
	
	if is_active_trap then
		sfx(5)
		game_over = true
	end
end

function check_flan()
	local is_flan =	mhas_flag(flags.flan,pl.x,pl.y)
	
	if is_flan then
		sfx(6)
		game_over = true
		player_win = true
	end
end
